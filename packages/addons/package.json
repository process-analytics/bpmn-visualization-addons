    "@jest/globals": "~29.7.0",
    "@testing-library/jest-dom": "~6.1.3",
    "cross-env": "~7.0.3",
    "eslint-plugin-jest": "~27.2.3",
    "eslint-plugin-jest-dom": "~5.1.0",
    "jest": "~29.7.0",
    "jest-environment-jsdom": "~29.7.0",
    "npm-run-all": "~4.1.5",
    "rimraf": "~5.0.1",
    "ts-jest": "~29.1.1"
  }
